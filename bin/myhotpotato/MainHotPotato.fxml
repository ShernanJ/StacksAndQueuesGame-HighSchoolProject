<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Text?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="myhotpotato.MainHotPotatoController">
   <center>
      <Pane id="mainWindow" prefHeight="500.0" prefWidth="500.0" stylesheets="@application.css" BorderPane.alignment="CENTER">
         <children>
            <Button id="mainButtons" layoutX="201.0" layoutY="413.0" mnemonicParsing="false" onAction="#openPlayerData" prefHeight="48.0" prefWidth="99.0" stylesheets="@application.css" text="Play" textAlignment="CENTER" />
            <Button id="mainButtons" layoutX="323.0" layoutY="413.0" mnemonicParsing="false" onAction="#openInstructions" prefHeight="48.0" prefWidth="130.0" stylesheets="@application.css" text="Instructions" textAlignment="CENTER" />
            <Button id="mainButtons" layoutX="75.0" layoutY="413.0" mnemonicParsing="false" onAction="#quitGame" prefHeight="48.0" prefWidth="99.0" stylesheets="@application.css" text="Quit" textAlignment="CENTER" />
            <ImageView fitHeight="253.0" fitWidth="284.0" layoutX="124.0" layoutY="104.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@hotPotato.jpg" />
               </image>
            </ImageView>
            <Text id="mainText" layoutX="99.0" layoutY="75.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Hot Potato Stacks and Queues" />
         </children>
      </Pane>
   </center>
</BorderPane>
